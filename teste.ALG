Algoritmo "semnome"

Var

    a, b, c : inteiro

Inicio
     a <- 10
     b <- 20
     c <- 15
     
     escreval (a>b)
     escreval (b<>c)
     escreval (c=a)
     
     escreval (a>=2)
     escreval(b<1)
     escreval (c=12)
     
     escreval (a=b+c)
     escreval (b<a+c)
     escreval (b>c+a+2)

Fimalgoritmo